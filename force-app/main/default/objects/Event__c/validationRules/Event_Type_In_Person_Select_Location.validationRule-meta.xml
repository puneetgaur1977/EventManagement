<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Event_Type_In_Person_Select_Location</fullName>
    <active>true</active>
    <errorConditionFormula>AND(
 NOT ISPICKVAL(Event_Type__c,&apos;Virtual&apos;) ,
 ISBLANK(Location__c) 
)</errorConditionFormula>
    <errorMessage>Event location should be  provided for In-Person event</errorMessage>
</ValidationRule>
